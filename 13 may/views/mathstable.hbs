<html>
    <body>
        <form action="generate-table" method="post">
            Enter Number to Show Table: <input type="number" name="num" />
            <input type="submit" value="Show Table" />
            <table>
                {{#each rowdata}}
                <tr>
                    <td>{{this}}</td>
                </tr>
                {{/each}}
            </table>
        </form>
    </body>
</html>